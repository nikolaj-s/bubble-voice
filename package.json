{
  "name": "bubble",
  "version": "0.6.2",
  "private": false,
  "dependencies": {
    "@popmotion/popcorn": "^0.4.4",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "analyser-frequency-average": "^1.0.0",
    "array-move": "^4.0.0",
    "axios": "^0.27.2",
    "browser-image-compression": "^2.0.0",
    "electron-updater": "^5.2.1",
    "framer-motion": "^6.3.3",
    "history": "^5.3.0",
    "iohook": "^0.9.3",
    "keytar": "^7.9.0",
    "mediasoup-client": "^3.6.52",
    "node-notifier": "^10.0.1",
    "popmotion": "^11.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-redux": "^8.0.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-twitter-embed": "^4.0.4",
    "react-youtube": "^10.0.0",
    "redux": "^4.2.0",
    "socket.io-client": "^4.5.1",
    "web-vitals": "^2.1.4"
  },
  "homepage": "./",
  "main": "electron/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "robocopy electron build/electron /S",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish always"
  },
  "build": {
    "appId": "com.bubble.voice",
    "productName": "Bubble",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "icon": "build/logo512.ico",
    "publish": {
      "provider": "github"
    },
    "win": {
      "icon": "build/logo512.ico",
      "target": "nsis",
      "publish": "github"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^12.2.3",
    "electron-builder": "^23.1.0"
  },
  "iohook": {
    "targets": [
      "node-88",
      "electron-87"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolaj-s/bubble-voice.git"
  },
  "publisher": "Nor. X West Designs"
}
